{
  "manifest_version": 3,
  "name": "RequestKeeper",
  "version": "1.0",
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": { "service_worker": "background.js" },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
